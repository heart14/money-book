<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.liiwe.moneybook.mapper.DiaryMapper">

    <insert id="batchInsertDiary" parameterType="java.util.List">
        INSERT INTO t_diary (`date`,`content`,`work_shift`,`username`,`create_at`)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.date}, #{item.content}, #{item.workShift}, #{item.username}, now())
        </foreach>
    </insert>

</mapper>